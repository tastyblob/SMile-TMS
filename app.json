{
    "expo": {
        "name": "Smile",
        "slug": "smile",
        "privacy": "public",
        "sdkVersion": "36.0.0",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "de.goodstag.smile",
            "versionCode": 1
        },
        "packagerOpts": {
            "config": "metro.config.js",
            "sourceExts": ["js", "jsx", "svg", "ts", "tsx"]
        }
    }
}
